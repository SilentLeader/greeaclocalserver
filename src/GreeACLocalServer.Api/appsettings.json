{
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            { 
                "Name": "Console",
                "Args": {
                    "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {ClientIPAddress} {Message:lj}{NewLine}{Exception}"
                }
            }
        ],
        "Enrich": [ "FromLogContext", "WithThreadId" ],
        "Properties": {
            "Application": "GreeACHeartBeatServer"
        }
    },
    "Server": {
        "Port": 5000,
        "DomainName": "local.server.name",
        "ExternalIp": "local.server.ip",
        "ListenIPAddresses": [],
        "CryptoKey": "a3K8Bx%2r8Y7#xDh",
        "EnableUI": true,
        "EnableManagement": true
    },
    "DeviceManager": {
        "DeviceTimeoutMinutes": 60
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://*:5100"
            }
        }
    },
    "ForwardedHeaders": {
        "ForwardedForHeaderName": "X-Forwarded-For",
        "ForwardedProtoHeaderName": "X-Forwarded-Proto",
        "ForwardedHostHeaderName": "X-Forwarded-Host",
        "RequireHeaderSymmetry": false,
        "KnownProxies": [],
        "KnownNetworks": [],
        "AllowedHosts": []
    }
}